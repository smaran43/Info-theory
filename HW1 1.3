#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main()
{
    int num_trials ;
    
    //Promting the user to give num_trials input
    
    scanf("%d", &num_trials);
    printf("Num_trials : %d\n", num_trials);
    srand(time(0));
    
    int n = num_trials;
    
    //Using for loop to generate num_trials number of random variables
    
    for( int i = 0 ; i < n ; i++ ){
        
        //Generating the random numbers
        
        double val = rand()%100000;
        double num_1 = val /100000 ; 
        
        //Sampling the random number to a
        
        if (num_1 <= 0.603648085911566 && num_1 > 0){
            printf("a");
        }
        
        //Sampling the random number to b
        
        if (num_1 <= 0.6878356883249475 && num_1 > 0.603648085911566){
            printf("b");
        }
        
        //Sampling the random number to c
        
        if (num_1 <= 0.7200789731798234 && num_1 > 0.6878356883249475){
            printf("c");
        }
        
        //Sampling the random number to d
        
        if (num_1 < 1 && num_1 > 0.7200789731798234){
            printf("d");
        }
    }
}
